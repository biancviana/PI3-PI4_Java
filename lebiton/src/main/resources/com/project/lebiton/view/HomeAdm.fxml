<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="549.0" prefWidth="629.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.project.lebiton.controller.HomeAdmController">
   <children>
      <Label layoutX="180.0" layoutY="40.0" text="ÁREA DO ADMINISTRADOR">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <Button fx:id="btCadastrar" layoutX="127.0" layoutY="125.0" mnemonicParsing="false" onAction="#cadastrarMedico" prefHeight="44.0" prefWidth="376.0" text="Cadastro de médico">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="btCadastroDias" layoutX="127.0" layoutY="195.0" mnemonicParsing="false" onAction="#cadastrarDiasAgenda" prefHeight="44.0" prefWidth="376.0" text="Cadastro de dias na agenda">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button id="btVoltar" fx:id="btDeslogar" layoutX="11.0" layoutY="11.0" mnemonicParsing="false" onMouseClicked="#deslogarOnClicked" prefHeight="27.0" prefWidth="81.0" text="Deslogar">
         <font>
            <Font size="13.0" />
         </font>
      </Button>
      <Button fx:id="btAgendaMedico" layoutX="127.0" layoutY="266.0" mnemonicParsing="false" onAction="#cadastrarAgendaMedico" prefHeight="44.0" prefWidth="376.0" text="Cadastro agenda do médico">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</Pane>
